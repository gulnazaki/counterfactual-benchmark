{
    "causal_graph": {
        "thickness": [],
        "intensity": ["thickness"],
        "image": ["thickness", "intensity"]
    },
    "mechanism_models": {
        "thickness": {
            "model_type": "flow",
            "model_class": "ThicknessFlow",
            "module": "models.flows",
            "params": {
                "layers": 3,
                "lr": 1e-3,
                "batch_size_train" : 64,
                "batch_size_val" : 10000,
                "patience" : 2,
                "max_epochs" : 1,
                "train_val_split" : 0.8
            }
        },
        "intensity": {
            "model_type": "flow",
            "model_class": "IntensityFlow",
            "module": "models.flows",
            "params": {
                "layers": 3,
                "lr": 1e-3,
                "batch_size_train" : 64,
                "batch_size_val" : 10000,
                "patience" : 2,
                "max_epochs" : 1,
                "train_val_split" : 0.8
            }
        },
        "digit": {
            "model_type": "flow",
            "model_class": "DigitFlow",
            "module": "models.flows",
            "params": {
                "layers": 3,
                "lr": 1e-3,
                "batch_size_train" : 64,
                "batch_size_val" : 10000,
                "patience" : 2,
                "max_epochs" : 1,
                "train_val_split" : 0.8
            }
        },
        "image": {
            "model_type": "vae",
            "model_class": "MmnistCondVAE",
            "module": "models.vaes",
            "params": {
                "n_chan" : [1, 16, 32, 128, 128],
                "latent_dim" : 32,
                "beta" : 4,
                "lr" : 1e-6,
                "gradient_clip_val": "null",
                "batch_size_train" : 128,
                "batch_size_val" : 256,
                "patience" : 10,
                "max_epochs" : 3,
                "train_val_split" : 0.8
            }
        }
    },
    "dataset": "morphomnist",
    "checkpoint_dir": "./checkpoints/"
}
