{
    "causal_graph": {
        "apoE": [],
        "age": [],
        "sex": [],
        "brain_vol": ["sex", "apoE", "age"],
        "vent_vol": ["brain_vol", "age"],
        "slice": [],
        "image": ["brain_vol", "vent_vol", "slice"]
    },
    "attribute_size": {
        "apoE": 2,
        "age": 1,
        "sex": 1,
        "brain_vol": 1,
        "vent_vol": 1,
        "slice": 20
    },
    "mechanism_models": {
        "brain_vol": {
            "model_type": "flow",
            "model_class": "BrainVolFlow",
            "module": "models.flows",
            "params": {
                "layers": 3,
                "lr": 0.005,
                "batch_size_train" : 64,
                "batch_size_val" : 10000,
                "patience" : 10,
                "max_epochs" : 1000
            }
        },
        "vent_vol": {
            "model_type": "flow",
            "model_class": "VentVolFlow",
            "module": "models.flows",
            "params": {
                "layers": 3,
                "lr": 0.005,
                "batch_size_train" : 64,
                "batch_size_val" : 10000,
                "patience" : 10,
                "max_epochs" : 1000
            }
        },
        "image": {
            "model_type": "vae",
            "model_class": "ADNICondVAE",
            "module": "models.vaes",
            "params": {
                "n_chan" : [1, 16, 24, 32, 64, 128, 256],
                "latent_dim" : 32,
                "hidden_dim": 512,
                "context_dim": 22,
                "beta" : 3,
                "lr" : 1e-3,
                "gradient_clip_val": 350,
                "batch_size_train" : 64,
                "batch_size_val" : 64,
                "patience" : 10,
                "max_epochs" : 1000,
                "train_val_split" : 0.8,
                "weight_decay": 0.01,
                "ema": "True",
                "fixed_logvar": "False"
            }
        }
    },
    "dataset": "adni",
    "checkpoint_dir": "./checkpoints/adni/trained_scm"
}
